    "notifications" : {
        "gBondLowUpRateCrossing" : {
            "nodetype" : "notification",
            "moduleName" : "GBOND-MIB",
            "oid" : "1.3.6.1.2.1.211.1.1.0.1",
            "status" : "current",
            "objects" : {
                "gBondPortStatUpDataRate" : {
